---
interface Props {
  href: string;
  downloadFileName: string;
}
import { FileText } from "astro-feather-icons";
const { href, downloadFileName } = Astro.props;
---

<a href={href} download={downloadFileName}>
  <button
    class="flex items-center text-sm gap-3 bg-indigo-100 hover:bg-opacity-80 dark:bg-white hover:dark:bg-opacity-90 text-indigo-500 dark:text-black py-2 px-4 rounded-lg font-semibold"
  >
    <FileText size="14" />
     Resume
  </button>
</a>
